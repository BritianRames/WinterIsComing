int a

fun fibo (int n){
        if(n == 0){
            return 0
        }

        if(n == 1){
            return 1
        } else{
            return fibo(n - 1) + fibo(n - 2)
        }
}

fun main (){
    int a
    a = 0
    int x
    x = 0
    while(x < 10){
        a = fibo(x)
        print(a)
        x++
    }

}
